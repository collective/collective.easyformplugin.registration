<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="object_buttons" meta_type="CMF Action Category">
  <object name="contact-registrants" meta_type="CMF Action" i18n:domain="collective.easyformplugin.registration">
   <property name="title" i18n:translate="">Contact registrants</property>
   <property name="description" i18n:translate=""></property>
   <property
      name="url_expr">python:object_url + '/@@contact-registrants'</property>
   <property name="icon_expr"></property>
   <property
      name="available_expr">python:getattr(object, 'is_registration_form', False)</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
